{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb","plugin:prettier/recommended"],
    "plugins": ["react","prettier"],
    "parserOptions": {
        "ecmaFeatures":{
            "jsx":true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "quotes": ["error", "double"],
        "prettier/prettier": ["warn",  {"endOfLine": "auto"}],
        "endOfLine": "off",
        "import/extensions": [
            "error",
            "always",
            {
              "js": "never",
              "jsx": "never",
              "ts": "never",
              "tsx": "never"
            }
         ]

    }
}
                                        